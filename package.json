{
  "name": "ncrf-gatelist",
  "version": "1.0.0",
  "description": "Gatelist site for Nor Cal Ren Faire",
  "repository": {
    "type": "git",
    "url": "https://github.com/DrSparkles/react-gatelist"
  },
  "license": "SEE LICENSE IN license.md",
  "scripts": {
    "client": "cd client && npm start",
    "install-and-run-client": "cd client && npm install && npm start",
    "start-server": "npm-run-all -s build server",
    "server": "nodemon build/server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run start-server\" \"npm run client\"",
    "build": "webpack --config webpack.config.js",
    "start": "concurrently --kill-others-on-fail \"npm run start-server\" \"npm run install-and-run-client\""
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "babel-loader": "^8.0.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "connect-history-api-fallback": "^1.5.0",
    "dotenv": "^6.0.0",
    "download-csv": "^1.1.1",
    "express": "^4.16.3",
    "immutable": "^3.8.2",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "mongodb": "^3.1.1",
    "monk": "^6.0.6",
    "morgan": "^1.9.0",
    "npm-run-all": "^4.1.3",
    "require-directory": "^2.1.1",
    "superagent": "^3.8.3",
    "superagent-promise": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "async": "^2.6.1",
    "chai": "^4.1.2",
    "concurrently": "^3.6.0",
    "mocha": "^5.2.0",
    "pow-mongodb-fixtures": "^0.14.0",
    "promise": "^8.0.1",
    "react-router": "^4.3.1",
    "sinon": "^6.1.3",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-node-externals": "^1.7.2"
  }
}
